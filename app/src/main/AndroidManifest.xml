<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="etec.com.ewen.matheus.appplantae">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppPlantae">
        <activity android:name=".activity.NotificacaoActivity"></activity>
        <activity android:name=".activity.TelaDiagnostico" />
        <activity android:name=".activity.TelaHelp" />
        <activity android:name=".activity.TelaGuias" />
        <activity android:name=".activity.TelaAlarme" />
        <activity android:name=".TelaAlteracao" />
        <activity
            android:name=".MenusActivity"
            android:label="@string/title_activity_menus"
            android:theme="@style/Theme.AppPlantae.NoActionBar" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.CadastroActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".AlertReceiver" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>